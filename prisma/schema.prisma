// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Orders {
  id String @id @map("_id")
  created_at DateTime
  totalPrice Float
  restaurant_id String
  user_id String
}

model Deliveries {
  id String @id @map("_id")
  order_id String
  created_at DateTime
  updated_at DateTime?
  restaurant_id String
}

model Users {
  id String @id @map("_id")
  created_at DateTime
}